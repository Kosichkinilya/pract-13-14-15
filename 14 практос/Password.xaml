<Window
    x:Class="_14_практос.Password"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:_14_практос"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="Password"
    Width="166"
    Height="121"
    Activated="Window_Activate"
    ResizeMode="NoResize"
    WindowStartupLocation="CenterOwner"
    WindowStyle="None"
    mc:Ignorable="d">
    
    <StackPanel>
        <PasswordBox x:Name="Pas" PasswordChar="*" />
        <Button
            x:Name="Enter"
            Click="Enter_Click"
            Content="Войти"
            IsDefault="True" />
        <Button
            x:Name="Cansel"
            Click="Cansel_Click"
            Content="Отмена"
            IsCancel="True" />
    </StackPanel>
</Window>
